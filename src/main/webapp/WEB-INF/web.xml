<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd" id="WebApp_ID" version="3.1">
  <welcome-file-list>
    <welcome-file>/index.jsp</welcome-file>
    <welcome-file>/egovDevIndex.jsp</welcome-file>
  </welcome-file-list>
  <login-config>
    <auth-method>BASIC</auth-method>
  </login-config>
  <session-config>
    <session-timeout>600</session-timeout>
    <tracking-mode>COOKIE</tracking-mode>
  </session-config>
  
  <error-page>
    <error-code>404</error-code>
    <location>/code404.jsp</location>
  </error-page>
  
  <error-page>
    <error-code>500</error-code>
    <location>/code500.jsp</location>
  </error-page>
  
  <error-page>
    <error-code>400</error-code>
    <location>/code400.jsp</location>
  </error-page>
  
  <error-page>
    <error-code>403</error-code>
    <location>/code403.jsp</location>
  </error-page>
  
  <error-page>
    <error-code>405</error-code>
    <location>/code405.jsp</location>
  </error-page>
  
  <error-page>
    <error-code>406</error-code>
    <location>/code406.jsp</location>
  </error-page>
  
  <error-page>
    <error-code>408</error-code>
    <location>/code408.jsp</location>
  </error-page>
  
  <error-page>
    <error-code>429</error-code>
    <location>/code429.jsp</location>
  </error-page>

  <error-page>
    <error-code>501</error-code>
    <location>/code501.jsp</location>
  </error-page>
</web-app>